<?xml version="1.0"?>
<robot name="ant">
    <material name="ant_material">
        <color rgba="0.8 0.6 0.4 1"/>
    </material>

    <link name="torso">
        <visual>
            <geometry>
                <sphere radius="0.25"/>
            </geometry>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>

    <!-- Front Left Leg -->
    <link name="front_left_leg">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="0.141 0.141 0" rpy="0 1.571 0.785"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="0.141 0.141 0" rpy="0 1.571 0.785"/>
        </collision>
    </link>

    <link name="front_left_foot">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="0.283 0.283 0" rpy="0 1.571 0.785"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="0.283 0.283 0" rpy="0 1.571 0.785"/>
        </collision>
    </link>

    <!-- Front Right Leg -->
    <link name="front_right_leg">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="-0.141 0.141 0" rpy="0 1.571 -0.785"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="-0.141 0.141 0" rpy="0 1.571 -0.785"/>
        </collision>
    </link>

    <link name="front_right_foot">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="-0.283 0.283 0" rpy="0 1.571 -0.785"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="-0.283 0.283 0" rpy="0 1.571 -0.785"/>
        </collision>
    </link>

    <!-- Back Left Leg -->
    <link name="left_back_leg">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="-0.141 -0.141 0" rpy="0 1.571 -2.356"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="-0.141 -0.141 0" rpy="0 1.571 -2.356"/>
        </collision>
    </link>

    <link name="left_back_foot">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="-0.283 -0.283 0" rpy="0 1.571 -2.356"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="-0.283 -0.283 0" rpy="0 1.571 -2.356"/>
        </collision>
    </link>

    <!-- Back Right Leg -->
    <link name="right_back_leg">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="0.141 -0.141 0" rpy="0 1.571 2.356"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.283"/>
            </geometry>
            <origin xyz="0.141 -0.141 0" rpy="0 1.571 2.356"/>
        </collision>
    </link>

    <link name="right_back_foot">
        <visual>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="0.283 -0.283 0" rpy="0 1.571 2.356"/>
            <material name="ant_material"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.08" length="0.566"/>
            </geometry>
            <origin xyz="0.283 -0.283 0" rpy="0 1.571 2.356"/>
        </collision>
    </link>

    <!-- Joints -->

    <!-- Front Left Leg Joints -->
    <joint name="hip_1" type="revolute">
        <parent link="torso"/>
        <child link="front_left_leg"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.698" upper="0.698" effort="150" velocity="1"/>
        <origin xyz="0.25 0.25 0"/>
    </joint>

    <joint name="ankle_1" type="revolute">
        <parent link="front_left_leg"/>
        <child link="front_left_foot"/>
        <axis xyz="-0.707 0.707 0"/>
        <limit lower="0.523" upper="1.745" effort="150" velocity="1"/>
        <origin xyz="0.283 0.283 0"/>
    </joint>

    <!-- Front Right Leg Joints -->
    <joint name="hip_2" type="revolute">
        <parent link="torso"/>
        <child link="front_right_leg"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.698" upper="0.698" effort="150" velocity="1"/>
        <origin xyz="-0.25 0.25 0"/>
    </joint>

    <joint name="ankle_2" type="revolute">
        <parent link="front_right_leg"/>
        <child link="front_right_foot"/>
        <axis xyz="0.707 0.707 0"/>
        <limit lower="-1.745" upper="-0.523" effort="150" velocity="1"/>
        <origin xyz="-0.283 0.283 0"/>
    </joint>

    <!-- Back Left Leg Joints -->
    <joint name="hip_3" type="revolute">
        <parent link="torso"/>
        <child link="left_back_leg"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.698" upper="0.698" effort="150" velocity="1"/>
        <origin xyz="-0.25 -0.25 0"/>
    </joint>

    <joint name="ankle_3" type="revolute">
        <parent link="left_back_leg"/>
        <child link="left_back_foot"/>
        <axis xyz="-0.707 0.707 0"/>
        <limit lower="-1.745" upper="-0.523" effort="150" velocity="1"/>
        <origin xyz="-0.283 -0.283 0"/>
    </joint>

    <!-- Back Right Leg Joints -->
    <joint name="hip_4" type="revolute">
        <parent link="torso"/>
        <child link="right_back_leg"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.698" upper="0.698" effort="150" velocity="1"/>
        <origin xyz="0.25 -0.25 0"/>
    </joint>

    <joint name="ankle_4" type="revolute">
        <parent link="right_back_leg"/>
        <child link="right_back_foot"/>
        <axis xyz="0.707 0.707 0"/>
        <limit lower="0.523" upper="1.745" effort="150" velocity="1"/>
        <origin xyz="0.283 -0.283 0"/>
    </joint>

</robot>